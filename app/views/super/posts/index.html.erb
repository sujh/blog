<section class="content">
  <div class="row">
    <%= render partial: 'super/shared/folders' %>

    <div class="col-md-9">
      <div class="box box-primary">
        <% @posts.each do |post| %>
          <%= render partial: 'post_display', locals: { post: post } %>
        <% end %>
        <%= paginate @posts %>
      </div>
    </div>
  </div>
</section>